   <to-view-id>/modules/mouvement/pages/OrdListe.xhtml</to-view-id>
   <redirect/>
  </navigation-case>
  <navigation-case>
   <from-outcome>ConListe</from-outcome>
   <to-view-id>/modules/mouvement/pages/ConListe.xhtml</to-view-id>
   <redirect/>
  </navigation-case>
  <navigation-case>
   <from-outcome>DepDetails</from-outcome>
   <to-view-id>/modules/mouvement/pages/DepDetails.xhtml</to-view-id>
